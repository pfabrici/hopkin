  <transform>
    <name>hopkin_dbparams: fetch exec details</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>hopkin</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
	lower(script_type)	AS script_type,
	CONCAT('src/',lower(job_folder),'/',jd.script_name, '.',jd.script_type)   AS script_fname,
	upper(is_delta_job)	AS is_delta_job,
	delta_script
FROM
	job_def   jd
WHERE 
	valid_from 		&lt;= CURRENT_DATE 
	AND valid_to 		> CURRENT_DATE 
	AND job_name 		= '${P_JOB_NAME}'   
	AND job_folder 	= '${P_JOB_FOLDER}'

</sql>
    <variables_active>Y</variables_active>
    <attributes/>
    <GUI>
      <xloc>896</xloc>
      <yloc>208</yloc>
    </GUI>
  </transform>
  <transform>
    <name>hopkin_dbparams: fetch job params</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>hopkin</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
	param_key,
	param_value,
	CASE 
		WHEN is_password ='N'
		THEN param_value 
		ELSE '***'
	END 		AS logger_value
FROM
	job_def  jd,
	job_param_group pf,
	job_param p
WHERE
	jd.job_name 		    	= '${P_JOBNAME}'
	AND jd.job_folder		= '${P_JOB_FOLDER}'
	AND jd.valid_from 		&lt;= CURRENT_DATE
	AND jd.valid_to 		> CURRENT_DATE
	AND jd.job_name        	= pf.job_name
	AND p.param_group    	= pf.param_group</sql>
    <variables_active>Y</variables_active>
    <attributes/>
    <GUI>
      <xloc>640</xloc>
      <yloc>208</yloc>
    </GUI>
  </transform>
