  <transform>
    <name>hopkin_dbparams: fetch exec details</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>hopkin_db</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
	script_type,
	CONCAT('src/',lower(job_type),'/',jd.script_name,CASE WHEN js.script_suffix IS NULL THEN '' ELSE js.script_suffix END,'.',jd.script_type)   AS script_fname,
	deltajob,
	deltascript
from
	( SELECT * FROM hopkin_meta.job_def   
	WHERE valid_from &lt;= CURRENT_TIMESTAMP AND valid_to > CURRENT_TIMESTAMP
	AND job_name = '${P_JOBNAME}'   ) jd
	LEFT JOIN ( SELECT * FROM hopkin_meta.job_suffix
	WHERE valid_from &lt;= CURRENT_TIMESTAMP AND valid_to > CURRENT_TIMESTAMP
	AND job_name = '${P_JOBNAME}'
	AND source_name = '${P_SOURCE}'
	) js
		ON ( jd.job_name = js.job_name )
</sql>
    <variables_active>Y</variables_active>
    <attributes/>
    <GUI>
      <xloc>896</xloc>
      <yloc>208</yloc>
    </GUI>
  </transform>
  <transform>
    <name>hopkin_dbparams: fetch job params</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>hopkin_db</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
	param_key,
	param_value,
	CASE 
		WHEN param_ispwd ='N'
		THEN param_value 
		ELSE '***'
	END 		AS logger_value
FROM
	hopkin_meta.job_def  jd,
	hopkin_meta.job_param_group pf,
	hopkin_meta.job_param p
WHERE
	jd.job_name 		    	= '${P_JOBNAME}'
	AND jd.valid_from 		&lt;= CURRENT_TIMESTAMP
	AND jd.valid_to 	> CURRENT_TIMESTAMP
	AND jd.job_name        = pf.job_name
	AND p.param_group    = pf.param_group</sql>
    <variables_active>Y</variables_active>
    <attributes/>
