    <connection>hopkin_db</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
	'${P_JOBNAME}'			AS job_name,
	CASE WHEN job_is_running IS NULL THEN 0 ELSE job_is_running END	AS job_is_running
FROM 
	( SELECT
 		1 AS jt,
		SUM(CASE WHEN unique_run = 'Y' THEN 1 ELSE 0 END)  AS job_is_running
	FROM
		hopkin_meta.job_prot  	p,
		hopkin_meta.job_def		d
	WHERE
		p.job_name = '${P_JOBNAME}'
		AND p.job_status = 'running'
		AND p.job_name = d.job_name
  	)  a
	-- simply create a row
	LEFT JOIN ( SELECT min(1) AS jt FROM hopkin_meta.job_def ) b
	ON ( b.jt = a.jt )</sql>
    <variables_active>Y</variables_active>
    <attributes/>
